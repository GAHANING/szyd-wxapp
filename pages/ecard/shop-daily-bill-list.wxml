<view class="page">
    <view class="page__hd">
        <view class="page__title">一卡通商户日账单</view>
        <view class="page__desc">{{name}}</view>
    </view>
    <view class="page__bd">
        <view class="weui-cells__title">日账单列表({{billsCount}})</view>
        <view class="weui-cells weui-cells_after-title">
            <navigator wx:for="{{bills}}" url="/pages/ecard/shop-daily-bill-detail?shopId=1&accdate=20180510" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd">{{item.accDate}}</view>
                <view class="weui-cell__ft weui-cell__ft_in-access">{{item.crAmtText}}</view>
            </navigator>
        </view>
    </view>
</view>