<view class="page">
	<view class="page__hd">
		<view class="page__title">虚拟化平台</view>
	</view>
	<view class="page__bd">
		<view class="weui-cells__title">运行中的虚拟机</view>
		<view class="weui-cells weui-cells_after-title">
			<navigator wx:for="{{datas1}}" wx:key="datas" class="weui-cell weui-cell_access" hover-class="weui-cell_active" url="/pages/fc/vm/vmone?vmUrn={{item.urn}}">
				<view class=" weui-cell__hd">
					<image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
				</view>
				<view class="weui-cell__bd">{{item.name}}</view>
				<view class="weui-cell__ft weui-cell__ft_in-access">{{item.vm.length}}</view>
			</navigator>
		</view>
	</view>
	<view class="page__bd">
		<view class="weui-cells__title">已停止的虚拟机</view>
		<view class="weui-cells weui-cells_after-title">
			<navigator wx:for="{{datas1}}" wx:key="datas" class="weui-cell weui-cell_access" hover-class="weui-cell_active" url="/pages/fc/vm/vmone?vmUrn={{item.urn}}">
				<view class=" weui-cell__hd">
					<image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
				</view>
				<view class="weui-cell__bd">{{item.name}}</view>
				<view class="weui-cell__ft weui-cell__ft_in-access">{{item.vm.length}}</view>
			</navigator>
		</view>
	</view>
</view>