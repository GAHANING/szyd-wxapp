<!--pages/bks/counter.wxml-->
<view class="page">
  <view class="page__hd">
    <view class="page__title">List</view>
    <view class="page__desc">本科生数量列表</view>
  </view>
  <view class="page__bd" wx:for="{{result}}" wx:key="date">
  <view class="weui-cells__title">按{{item.name}}分类</view>
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell" wx:for="{{item.list}}" wx:key="date">
        <view class="weui-cell__bd">{{item.name}}数量</view>
        <view class="weui-cell__ft">{{item.counter}}人</view>
      </view>
    </view>
  </view>
</view>